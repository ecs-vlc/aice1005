#include "binary_tree.h"

int main() {
  binary_tree<int> tree;
  tree.insert(5);
  tree.insert(3);
  tree.insert(4);
  tree.insert(8);
  tree.insert(10);
  tree.insert(9);
  tree.insert(2);
  tree.insert(3);

  cout << tree.find(5).second << " " << tree.find(7).second << " " << tree.find(3).second << endl;
  tree.show_tree();
  tree.print_tree();
  int cnt = 0;
  for (int element: tree) {
    cout << element << ' ';
  }
  cout << endl;
  
  for (auto pt=tree.find(5).first; pt!=tree.end(); ++pt) {
    if (++cnt==10)
      break;
    cout << *pt << ' ';
  }
  cout << endl;
}
